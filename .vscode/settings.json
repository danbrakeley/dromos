{
  "prettier.documentSelectors": [
    "*.json",
    "*.yml",
    "*.yaml",
    "*.md"
  ],
  "prettier.printWidth": 120,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "bsdiff",
    "clippy",
    "Dromos",
    "headered",
    "headerless",
    "hexdigit",
    "pathfinding",
    "petgraph",
    "reimplementation",
    "reimplementations",
    "rusqlite",
    "rustyline",
    "thiserror",
    "walkdir",
    "xdelta"
  ],
}
